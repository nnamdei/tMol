{
  "rules": {
    "no-console": 0,
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "comma-dangle": "off",
    "object-curly-newline": "off",
    "no-underscore-dangle": 0,
    "no-unused-vars": ["error", {
      "argsIgnorePattern": "next"
    }],
    "no-use-before-define": ["error", {
      "variables": false
    }],
    "no-multi-str": 0
  },
  "env": {
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "extends": [
    "airbnb-base"
  ]
}
